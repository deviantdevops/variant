{
  "name": "@deviantcode/variant",
  "version": "1.0.0",
  "description": "Variant Private & Anonymous Coin Driven Communication Systems",
  "main": "index.js",
  "scripts": {
    "test": "jest --watchAll",
    "dev": "cross-env NODE_ENV=development nodemon ./index.js",
    "dev-peer": "cross-env GENERATE_PEER_PORT='true' NODE_ENV=development nodemon ./index.js",
    "prod": "cross-env NODE_ENV=production node ./index.js",
    "prod-peer": "cross-env GENERATE_PEER_PORT='true' NODE_ENV=production nodemon ./index.js",
    "build": "parcel build client/src/index.html --out-dir client/dist",
    "build:dev": "npm run clean && cross-env NODE_ENV=development REACT_APP_ENV=development npm run build",
    "build:prod": "npm run clean && cross-env NODE_ENV=production REACT_APP_ENV=production npm run build",
    "build:staging": "npm run clean && cross-env NODE_ENV=staging REACT_APP_ENV=staging npm run build",
    "clean": "rm -rf client/dist/*",
    "postbuild": "gzipper --verbose client/dist/ ",
    "watch": "nodemon -e js,css --watch client/src --exec \"npm run build:dev && cross-env NODE_ENV=development REACT_APP_ENV=development nodemon ./index.js\" "
  },
  "author": "@Variant",
  "license": "ISC",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "gzipper": "^2.10.4",
    "jest": "^23.6.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "axios": "^0.21.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "base-64": "^1.0.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "env": "^0.0.2",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "hex-to-binary": "^1.0.1",
    "ioredis": "^4.27.6",
    "js-cookie": "^2.2.1",
    "js-sha256": "^0.9.0",
    "js-sha3": "^0.8.0",
    "lmdb": "^1.5.5",
    "node-lmdb": "^0.9.4",
    "parcel-bundler": "^1.12.5",
